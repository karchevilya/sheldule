<div align="center">
  <h2 class="display-3">Shedule</h2>
</div>

<table class="table table-striped" border="2">
  <thead class="thead-dark">
    <tr>
      <th colspan="2"><%= @item.day %></th>
    </tr>
  </thead>
  <tr>
    <td>08:00-09:50</td>
    <td><%= @item.st1 %></td>
  </tr>
  <tr>
    <td>09:50-11:25</td>
    <td><%= @item.st2 %></td>
  </tr>
  <tr>
    <td>11:40-13:15</td>
    <td><%= @item.st3 %></td>
  </tr>
  <tr>
    <td>13:45-15:20</td>
    <td><%= @item.st4 %></td>
  </tr>
  <tr>
    <td>15:35-17:10</td>
    <td><%= @item.st5 %></td>
  </tr>
  <tr>
    <td>17:25-19:00</td>
    <td><%= @item.st6 %></td>
  </tr>
</table>

<div align="center">
  <table>
    <tr>
      <% if user_signed_in? %>
        <td><%= button_to 'Edit', edit_item_path, method: :get%></td>
      <% end %>
    </tr>
  </table>
</div>
<br/>

<%= render partial: 'layouts/shared/group'%>


