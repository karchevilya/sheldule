<div align="center">
  <h2 class="display-3">Shedule</h2>
</div>

<div align="center">
  <h2 class="display-5"><%= @group %></h2>
</div>

<table class="table table-striped" border="2">
  <thead class="thead-dark">
    <tr>
      <th></th>
      <% @items.each do |item| %>
        <th><%= link_to item.day, item_path(item)%></th>
      <% end %>
    </tr>
  </thead>

  <tr>
    <td>08:00-09:50</td>
    <% @items.each do |item| %>
      <td><%= item.st1 %></td>
    <% end %>
  </tr>
  <tr>
    <td>09:50-11:25</td>
    <% @items.each do |item| %>
      <td><%= item.st2 %></td>
    <% end %>
  </tr>
  <tr>
    <td>11:40-13:15</td>
    <% @items.each do |item| %>
      <td><%= item.st3 %></td>
    <% end %>
  </tr>
  <tr>
    <td>13:45-15:20</td>
    <% @items.each do |item| %>
      <td><%= item.st4 %></td>
    <% end %>
  </tr>
  <tr>
    <td>15:35-17:10</td>
    <% @items.each do |item| %>
      <td><%= item.st5 %></td>
    <% end %>
  </tr>
  <tr>
    <td>17:25-19:00</td>
    <% @items.each do |item| %>
      <td><%= item.st6 %></td>
    <% end %>
  </tr>
</table>
<br/>

<%= render partial: 'layouts/shared/group'%>

